<%- include('partials/header') %>
<div class="container">
     <p style="margin-left: 32%;margin-top: 60px; font-size: 50px;"><b>Create New Camps</b></p>
     <div style="width: 30%; margin:auto">
        <form action="/campgrounds/new" method="POST" style="margin-top: 10px;" >
        <div class="form-group">
            <input class="form-control" type="text" name="title" placeholder="Enter Name" autocomplete="off">
        </div>
        <div class="form-group">
            <input class="form-control" type="text" name="image" placeholder="Enter URL" autocomplete="off">
        </div>
        <div class="form-group">
            <input class="form-control" type="text" name="description" placeholder="Description!" autocomplete="off">
        </div>
            <button class="btn btn-lg btn-block btn-dark" style="margin-top: 10px;margin-bottom: 10px;" type="submit">Submit</button>
        </form>
    </div>
    <div >
     <a style ="margin-left: 47%;" href="/campgrounds">Go back</a>
     <p style="margin-left:42%;">Trademark Yelpcamp 2015</p>
    </div>
</div>

<%- include('partials/footer') %>
